@model Doctor?

@{
    ViewData["Title"] = "Editar Médico";
    Layout = "_Layout";
    ViewBag.Action = "Edit";
}

@if (Model is not null)
{
    @await Html.PartialAsync("DoctorFormPartial")
}
else
{
    <div class="text-center">
        <h3>Médico não encontrado</h3>
    </div>
}

<div class="form-group">
    <a asp-controller="Doctor" asp-action="Index" class="btn btn-secondary">Voltar</a>
</div>