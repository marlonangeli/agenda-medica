@model Patient?

@{
    ViewBag.Title = "Editar Paciente";
    Layout = "_Layout";
}

@if (Model is not null)
{
    @await Html.PartialAsync("PatientFormPartial", (Model, "Edit"))
}
else
{
    <div class="text-center">
        <h3>Paciente não encontrado</h3>
    </div>
}

@section Scripts {
    <script>
        $(document).ready(function () {
            $("#CPF").mask("999.999.999-99");
            $("#Phone").mask("(99) 99999-9999");
        });
    </script>
}
